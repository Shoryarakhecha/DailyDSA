Rotate Array


class Solution {
    // Function to rotate an array by d elements in counter-clockwise direction.
    static void rotate(int si,int ei,int[] arr){
        while(si<ei){
            int temp=arr[si];
            arr[si]=arr[ei];
            arr[ei]=temp;
            si++;
            ei--;
        }
    }
    static void rotateArr(int arr[], int d) {
        // add your code here
        int n=arr.length;
        d=d%n;
        rotate(0,d-1,arr);
        rotate(d,n-1,arr);
        rotate(0,n-1,arr);
    }
}
