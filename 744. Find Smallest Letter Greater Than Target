744. Find Smallest Letter Greater Than Target

class Solution {
    public char nextGreatestLetter(char[] letters, char target) {
        int max=-1;
        for(int i=0;i<letters.length;i++){
            if((target-'a')<(letters[i]-'a')){
                return letters[i];
            }
        }
        return letters[0];
    }
}
