medium-->714. Best Time to Buy and Sell Stock with Transaction Fee
memoization solution-->
class Solution {
    public int dp[][];
    public int helper(int arr[],int indx,int buy,int fee){
        if(indx>=arr.length) return 0;
        if(dp[indx][buy]!=-1) return dp[indx][buy];
        int profit=0;
        if(buy==1){
            profit= Math.max(-arr[indx]+helper(arr,indx+1,0,fee),0+helper(arr,indx+1,1,fee));
        }else{
            profit= Math.max((arr[indx]-fee)+helper(arr,indx+1,1,fee),0+helper(arr,indx+1,0,fee));
        }
        return dp[indx][buy]=profit;

    }
    public int maxProfit(int[] prices,int fee) {
        dp=new int [prices.length+1][2];
        for(int row[]:dp) Arrays.fill(row,-1);
        return helper(prices,0,1,fee);

        
    }
} 
