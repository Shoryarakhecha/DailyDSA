190. Reverse Bits
class Solution {
    public int reverseBits(int n) {
        String str=Integer.toBinaryString(n);
        while(str.length()<32){
            str="0"+str;
        }
        StringBuilder sb=new StringBuilder(str);
        return (int) Long.parseLong(sb.reverse().toString(),2);
    }
}
