1925. Count Square Sum Triples-->https://leetcode.com/problems/count-square-sum-triples/description/?envType=daily-question&envId=2025-12-08

class Solution {
    public int countTriples(int n) {
        int count=0;
        for(int i=1;i<n;i++){
            for(int b=i+1;b<n;b++){
                int sumSqr=i*i +b*b;
                int c=(int)Math.sqrt(sumSqr);

                if(c*c==sumSqr && c<=n){
                    count+=2;
                }
            }

        }
        return count;
    }
}
