53. Maximum Subarray

class Solution {
    public int maxSubArray(int[] nums) {
        int max=nums[0];
        int curMax=0;
        for(int i:nums){
            if(curMax<0){
                curMax=0;
            }
            curMax+=i;

            max=Math.max(curMax,max);
        }
        return max;
        
    }
}
