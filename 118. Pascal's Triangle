118. Pascal's Triangle

class Solution {
    public List<List<Integer>> generate(int n) {
        List<List<Integer>>ans=new ArrayList<>();
        for(int i=1;i<=n;i++){
            List<Integer>li=new ArrayList<>();
            int c=1;
            for(int j=1;j<=i;j++){
                li.add(c);
                c=((i-j)*c)/j;
            }
            ans.add(new ArrayList<>(li));
        }
        return ans;
    }
}
