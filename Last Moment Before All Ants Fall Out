Last Moment Before All Ants Fall Out


class Solution {
    public int getLastMoment(int n, int left[], int right[]) {
        // code here
        int leftMax=0;
        for(int i:left){
            leftMax=Math.max(i,leftMax);
        }
        int rightMax=0;
        for(int i:right){
            rightMax=Math.max(rightMax,n-i);
        }
        return Math.max(leftMax,rightMax);
        
    }
}
