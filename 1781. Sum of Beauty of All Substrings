1781. Sum of Beauty of All Substrings

class Solution {
    public int beautySum(String s) {
        int ans = 0;
        for (int i = 0; i < s.length(); i++) {
            int freq[] = new int[26];
            int max = -1;
            freq[s.charAt(i) - 'a']++;
            
            for (int j = i + 1; j < s.length(); j++) {
                freq[s.charAt(j) - 'a']++;
                
                int min = Integer.MAX_VALUE;
                max = -1;
                for (int ele : freq) {
                    if (ele != 0) {
                        min = Math.min(min, ele);
                        max = Math.max(max, ele);
                    }
                }
                ans += max - min;
            }
        }
        return ans;
    }
}
